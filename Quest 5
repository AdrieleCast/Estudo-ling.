/Leia uma matriz 3x3 e mostre quantos números são pares./

#include <stdio.h>

int main(){

int m[3][3], i, j, qnt = 0;

for(i = 0; i < 3; i++) {
for(j = 0; j < 3; j++){
scanf("%d", &m[i][j]);
}
}

for(i = 0; i < 3; i++) {
for(j = 0; j < 3; j++){
if(m[i][j] % 2 == 0){

qnt++;  
    
  }  
 }

}
printf("%d", qnt);

return 0;
}
